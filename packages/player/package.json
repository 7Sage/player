{
  "name": "@vidstack/player",
  "version": "1.5.2",
  "description": "Vidstack Player: Design a modern media player only using HTML and CSS.",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist/",
    "define/",
    "index.d.ts",
    "globals.d.ts",
    "elements.json",
    "tailwind.cjs",
    "vscode.html-data.json"
  ],
  "sideEffects": [
    "./dist/**/define/*.js"
  ],
  "exports": {
    ".": {
      "node": "./dist/node/index.js",
      "development": "./dist/dev/index.js",
      "default": "./dist/prod/index.js"
    },
    "./define/*": {
      "node": "./dist/node/define/*",
      "development": "./dist/dev/define/*",
      "default": "./dist/prod/define/*"
    },
    "./cdn/define/*": {
      "default": "./dist/cdn/define/*"
    },
    "./package.json": "./package.json",
    "./tailwind.cjs": "./tailwind.cjs",
    "./elements.json": "./elements.json",
    "./vscode.html-data.json": "./vscode.html-data.json"
  },
  "contributors": [
    "Dave Furfero <furf@furf.com>",
    "Rahim Alwer <rahim.alwer@gmail.com>",
    "Sam Potts <sam@potts.es>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vidstack/vidstack.git",
    "directory": "packages/player"
  },
  "bugs": {
    "url": "https://github.com/vidstack/vidstack/issues"
  },
  "scripts": {
    "analyze": "eliza \"src/**/*{events,Element}.ts\"",
    "build:dist": "rollup -c",
    "build:definitions": "node ../../.scripts/definitions.js",
    "build:types": "tsc -p tsconfig.json --pretty && api-extractor run -c types.json && rimraf dist/**/*.d.ts",
    "clean": "rimraf define dist coverage",
    "dev": "run-p watch:dist watch:analyze watch:types",
    "lint:eslint": "eslint src --fix",
    "lint:prettier": "prettier src --write --loglevel warn",
    "minify:cdn": "node ../../.scripts/minify.js --entry=dist/cdn/**/*.js --outdir=dist/cdn",
    "sandbox": "pnpm sandbox:prepare && vite --open /sandbox/index.html --port 3101",
    "sandbox:prepare": "node ../../.scripts/copy.js --entry=.templates/sandbox --outdir=sandbox --overwrite=false",
    "test": "vitest --run",
    "watch:dist": "pnpm run build:dist -w",
    "watch:analyze": "pnpm run analyze -w",
    "watch:types": "node ../../.scripts/run-on-change.js --glob=\"src/**/*.ts\" --scripts=build:types",
    "watch:test": "vitest --watch"
  },
  "dependencies": {
    "@vidstack/foundation": "workspace:*"
  },
  "peerDependencies": {
    "lit": "^2.1.1"
  },
  "devDependencies": {
    "@lit/reactive-element": "^1.3.1",
    "@microsoft/api-extractor": "^7.20.0",
    "@open-wc/semantic-dom-diff": "^0.19.5",
    "@open-wc/testing-helpers": "^2.1.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/lru-cache": "^7.4.0",
    "@vidstack/eliza": "workspace:*",
    "@vidstack/rollup": "workspace:*",
    "autoprefixer": "^10.4.2",
    "hls.js": "^1.1.3",
    "jsdom": "^19.0.0",
    "lit": "^2.1.3",
    "lit-html": "^2.2.1",
    "lru-cache": "^7.4.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.6",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "tailwindcss": "^3.0.22",
    "tslib": "^2.3.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.1",
    "vitest": "^0.6.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "accessible",
    "audio",
    "customizable",
    "dailymotion",
    "elements",
    "extensible",
    "file",
    "framework-agnostic",
    "hls",
    "html5",
    "lit",
    "media",
    "plyr",
    "player",
    "spec-compliant",
    "universal",
    "vds",
    "video",
    "vidstack",
    "vime",
    "vimeo",
    "web-component",
    "universal",
    "youtube"
  ]
}
